'''
class Chair:
    def __init__(self,name):
        self.jackchair=name
        print(self.jackchair)
        
jackchair=Chair("jack's chair")

'''


'''
class Chair:
    all_chairs=[]
    def __init__(self,name):
        self.name=name
        self.all_chairs.append(name)

jack_chair=Chair("Jacks chair")
pat_chair=Chair("Pats chair")
print(pat_chair.all_chairs)
'''


'''
class Example:
    def __init__(self,a):
        self.a=a
        
        
a=Example(int(5))
print(a)
'''
'''
class Test:
    def fun(self):
        print("fun")
    def hello(self):    

        print("hello")
obj=Test()
obj.fun()
obj.hello()      
'''




















































































































































































































































































































































































































































































































































































































